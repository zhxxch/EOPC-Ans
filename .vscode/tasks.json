{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "make",
			"type": "shell",
			"command": "import-vs;nmake",
			"problemMatcher": ["$msCompile"],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "make all",
			"type": "shell",
			"command": "import-vs;nmake /all",
			"problemMatcher": ["$msCompile"],
			"group": "build"
		}
	]
}